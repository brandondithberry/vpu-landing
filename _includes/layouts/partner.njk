---
layout: layouts/base.njk
section: partner
---

<article>
  <h1>{{ title }}</h1>
  <p>
    <small>
      <time datetime="{{ page.date | machineDate }}">{{
        page.date | readableDate
      }}</time>
      by <a href="/authors/{{ author | slug }}/">{{ author }}</a>
    </small>
  </p>

  {{ layoutContent | safe }}

  {% if tags %}
  <p>
    {% for tag in tags %} {%- if tag != "partner" -%} {% set tagUrl %}/tags/{{
      tag
    }}/{% endset %}
    <a href="{{ tagUrl | url }}" rel="tag">{{ tag }}</a>
    {%- endif -%} {% endfor %}
  </p>
  {% endif %}
</article>

<nav>
  <a href="{{ '/partner/' | url }}">â† Blog index</a>
</nav>
